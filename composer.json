{
  "name": "abdalasif/geo-timezone",
  "description": "PHP library for timezone calculation from a particular location and date.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/phayes/geoPHP",
      "autoload": {
        "files": [
          "vendor/phayes/geophp/geoPHP.inc"
        ]
      }
    }
  ],
  "require": {
    "php": ">=7.0 || >=8.0",
    "ext-dom": ">=7.0 || >=8.0",
    "ext-mbstring": ">=7.0 || >=8.0",
    "ext-zip": "*",
    "ext-xml":">=7.0 || >=8.0",
    "phayes/geophp": "1.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.5",
    "guzzlehttp/guzzle": "7.8"
  },
  "autoload": {
    "psr-4": {
      "GeoTimeZone\\": "src/GeoTimeZone/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
